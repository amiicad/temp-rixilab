            <td class="text-center">
              <% if (i.completionSent) { %>
                <input type="checkbox" disabled>
              <% } else { %>
                <input type="checkbox" name="interns[]" value="<%= i.intern_id %>" class="completionCheckbox form-check-input" onclick="updateRowColor(this, 'completion')">
              <% } %>
            </td>
          </tr>
        <% }) %>
      </tbody>
    </table>
  </form>
